function assume-role() {
  aws sts assume-role \
    --role-session-name stv-test \
    --role-arn arn:aws:iam::$1:role/stv-test \
    --query "Credentials"
}
function get-ssm-param() {
  aws ssm get-parameter --region us-east-1 \
    --name $1 \
    --with-decryption \
    --output json
}

